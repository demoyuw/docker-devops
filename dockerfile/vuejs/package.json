{
  "name": "jenesius-vue-modal",
  "version": "1.0.14",
  "private": false,
  "description": "Simple modal plugin for Vue3",
  "author": "Jenesius",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "plugin/index.js",
  "files": [
    "plugin/*"
  ],
  "dependencies": {
    "vue": "^3.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-rc.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "email": "lokargenia@gmail.com",
  "homepage": "https://modal.jenesius.com",
  "keywords": [
    "vue",
    "vue3",
    "modal",
    "dialog"
  ],
  "license": "MIT",
  "url": "https://github.com/Jenesius/vue-modal/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jenesius/vue-modal.git"
  }
}
